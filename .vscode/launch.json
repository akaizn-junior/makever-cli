    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "makever default -c",
            "program": "${workspaceFolder}/src/CLI.js",
            "args": ["-f"],
            "console": "integratedTerminal",
            "trace": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "makever -o",
            "program": "${workspaceFolder}/src/CLI.js",
            "args": ["-f", "-o", "output"],
            "console": "integratedTerminal",
            "trace": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "makever --tag",
            "program": "${workspaceFolder}/src/CLI.js",
            "args": ["--tag", "-m", "Codename %c for Version %v"],
            "console": "integratedTerminal",
            "trace": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "makever -v",
            "program": "${workspaceFolder}/src/CLI.js",
            "args": ["-f", "-v", "prerelease --preid=%c", "-m", "Codename %c for Version %v"],
            "console": "integratedTerminal",
            "trace": true
        }
    ]
}
