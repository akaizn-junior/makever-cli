{
    "name": "makever",
    "version": "12.1.1-cooperative-fired.13",
    "description": "Creates a file with more descriptive information based on the version of your package",
    "main": "src/CLI.js",
    "scripts": {
        "test": "nyc mocha tests/*.spec.js",
        "test:w": "nodemon --exec 'npm run test'",
        "test:f": "nodemon --exec 'nyc mocha tests/*.spec.js --grep'",
        "lint": "eslint .",
        "lint:w": "nodemon --exec 'npm run lint'"
    },
    "bin": {
        "makever": "src/CLI.js"
    },
    "author": {
        "name": "Simao Nziaka",
        "email": "sdnziaka@gmail.com"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "lines": 10,
        "statements": 10,
        "functions": 10,
        "branches": 0,
        "reporter": [
            "html",
            "text-summary"
        ],
        "exclude": [
            "**/*.spec.js"
        ],
        "extension": [
            ".spec.js"
        ],
        "cache": true,
        "all": false
    },
    "license": "ISC",
    "keywords": [
        "make",
        "version",
        "package.json",
        "package",
        "file",
        "codename",
        "tag",
        "git",
        "makever"
    ],
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "chai": "^4.2.0",
        "eslint": "^6.5.1",
        "mocha": "^6.2.1",
        "nodemon": "^1.19.3",
        "nyc": "^14.1.1",
        "sinon": "^7.5.0"
    },
    "dependencies": {
        "semver": "^6.3.0"
    }
}
